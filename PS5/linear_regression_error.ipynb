   "source": [
    "Ein_threshold = 0.008\n",
    "sigma = 0.1\n",
    "d = 8"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 11,
   "metadata": {},
   "outputs": [],
   "source": [
    "def lin_reg_error(sigma_in, d_in, N):\n",
    "    return sigma_in * sigma_in * (1 - (d_in + 1) / N)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 12,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Minimum N: 45\n"
     ]
    }
   ],
   "source": [
    "for i in range(1, 1000):\n",
    "    if lin_reg_error(sigma, d, i) > Ein_threshold:\n",
    "        print(f\"Minimum N: {i}\")\n",
    "        break"
   ]
  }
